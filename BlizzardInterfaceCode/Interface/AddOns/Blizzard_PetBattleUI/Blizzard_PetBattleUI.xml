					<Anchors>
						<Anchor point="CENTER" relativeKey="$parent.Framing" relativePoint="CENTER" x="1" y="-15"/>
					</Anchors>
					<Color r="0" g="0" b="0" a="0.5"/>
				</Texture>
			</Layer>
